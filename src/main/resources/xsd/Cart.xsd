<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Carts">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" name="Cart">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="accountId" type="xs:unsignedByte" />
                            <xs:element name="productId" type="xs:unsignedByte" />
                            <xs:element name="name" type="xs:string" />
                            <xs:element name="quantity" type="xs:unsignedByte" />
                            <xs:element name="image" type="xs:string" />
                            <xs:element name="price" type="xs:unsignedByte" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>